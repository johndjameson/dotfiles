snippet job "jQuery Object"
$( '$1' )$0
endsnippet

snippet jon "jQuery 'on'" i
.on '${1:click}', ${2:(event) }->
	$0
endsnippet

snippet jor "jQuery Document Ready"
jQuery ($) ->
	$0
endsnippet

snippet jpd "jQuery Prevent Default"
${1:event}.preventDefault()$0
endsnippet

snippet jt "jQuery This"
$(@)$0
endsnippet
