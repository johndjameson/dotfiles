#!/usr/bin/env bash
# ****************************************************
#
#   Download Zip
#   -> Download and extract an external ZIP archive
#
# ****************************************************

set -o errexit
set -o nounset
set -o pipefail

# ----------------------------------------------------
#   Main
# ----------------------------------------------------

# -L must come before -o to follow redirection
curl -s -L -o .tempzip "$1"
unzip -q -u .tempzip
rm .tempzip
